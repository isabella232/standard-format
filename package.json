{
  "name": "videojs-standard-format",
  "version": "1.3.6",
  "description": "attempts to reformat javascript to comply with videojs/standard style",
  "main": "index.js",
  "bin": {
    "standard-format": "./bin.js",
    "videojs-standard-format": "./bin.js"
  },
  "scripts": {
    "test": "standard && tape test/*.js | tap-spec",
    "test-file": "<test.js ./bin.js | standard",
    "failing": "standard && tape test/failing/*.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/videojs/standard-format.git"
  },
  "author": "max ogden",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/videojs/standard-format/issues"
  },
  "homepage": "https://github.com/videojs/standard-format",
  "dependencies": {
    "esformatter": "^0.6.0",
    "esformatter-eol-last": "^1.0.0",
    "esformatter-literal-notation": "^1.0.0",
    "esformatter-quotes": "^1.0.0",
    "esformatter-spaced-lined-comment": "^2.0.0",
    "find-root": "^0.1.1",
    "glob": "^5.0.5",
    "minimatch": "^2.0.1",
    "minimist": "^1.1.0",
    "stdin": "0.0.1"
  },
  "devDependencies": {
    "concat-stream": "^1.4.7",
    "debug": "^2.1.1",
    "once": "^1.3.1",
    "skip-stream": "0.0.3",
    "split": "^0.3.3",
    "videojs-standard": "*",
    "stream-reduce": "^1.0.3",
    "tap-spec": "^2.2.2",
    "tape": "^3.5.0"
  },
  "standard": {
    "ignore": [
      "test.js",
      "**test/failing/**"
    ]
  }
}
